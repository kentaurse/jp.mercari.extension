{
  "name": "jp.mercari.extension",
  "description": "List of users who liked jp.mercari's products",
  "version": "0.0.1",
  "manifest_version": 3,
  "icons": {
      "16": "logo16.png",
      "48": "logo48.png",
      "128": "logo128.png"
  },
  "action": {
      "default_popup": "index.html"
  },
  "permissions": ["scripting", "activeTab", "tabs", "storage"],
  "host_permissions": ["<all_urls>"],
  "homepage_url": "https://jp.mercari.com/"
}
